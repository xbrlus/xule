{
    "name": "xule",
    "displayName": "XULE",
    "description": "XULE Language Pack",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "xule",
