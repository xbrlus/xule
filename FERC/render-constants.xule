					   else duration(date($currentYear + '-' + pad(($currentMonth.number - 2).string, 2, '0') + '-01'),
							         date($currentYear + '-' + pad(($currentMonth.number - 2).string, 2, '0') + '-' + $monthEndList[$currentMonth.number - 2]))

