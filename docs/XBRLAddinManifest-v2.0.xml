                  </Icon>

                  <!-- This is what happens when the command is triggered (E.g. click on the Ribbon). Supported actions are ExecuteFunction or ShowTaskpane. -->
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>showDocumentationURL</FunctionName>
                  </Action>
                </Control>
                <Control xsi:type="Button" id="XBRL.Community">
                  <Label resid="XBRL.Community.Label" />
                  <Supertip>
                    <!-- ToolTip title. resid must point to a ShortString resource. -->
                    <Title resid="XBRL.Community.Label" />
                    <!-- ToolTip description. resid must point to a LongString resource. -->
                    <Description resid="XBRL.Community.Tooltip" />
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="XBRL.community_16x16" />
                    <bt:Image size="32" resid="XBRL.community_32x32" />
                    <bt:Image size="80" resid="XBRL.community_80x80" />
                  </Icon>

                  <!-- This is what happens when the command is triggered (E.g. click on the Ribbon). Supported actions are ExecuteFunction or ShowTaskpane. -->
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>showCommunityURL</FunctionName>
                  </Action>
                </Control>


              </Group>


              <Label resid="XBRLCustomTab.Label" />
            </CustomTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    
    
    
